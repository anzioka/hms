                            </rowConstraints>
                            <children>
                                <Label text="DATE ADMITTED">
                                    <styleClass>
                                        <String fx:value="fw-500" />
                                        <String fx:value="color-dark-2" />
                                    </styleClass>
                                </Label>
                                <Label text="DATE DISCHARGED" GridPane.rowIndex="1">
                                    <styleClass>
                                        <String fx:value="fw-500" />
                                        <String fx:value="color-dark-2" />
                                    </styleClass>
                                </Label>
                                <Label fx:id="dateAdmitted" text="Label" GridPane.columnIndex="1" />
                                <Label fx:id="dateDischarged" text="-" GridPane.columnIndex="1" GridPane.rowIndex="1" />
                            </children>
                        </GridPane>
                    </children>
                </HBox>
                <HBox alignment="CENTER_LEFT" prefWidth="564.0" spacing="10.0">
