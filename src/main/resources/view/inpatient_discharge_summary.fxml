                        <Label fx:id="diagnosis" text="diagnosis" wrapText="true" />
                    </children>
                    <padding>
                        <Insets bottom="5.0" top="5.0" />
                    </padding>
                </VBox>
                <VBox layoutX="15.0" layoutY="405.0" minHeight="40.0" prefWidth="564.0" spacing="5.0">
                    <children>
                        <Label text="PROCEDURES:">
                            <styleClass>
                                <String fx:value="fw-500" />
                                <String fx:value="color-dark-2" />
                            </styleClass>
                        </Label>
                        <Label fx:id="procedures" text="procedures" wrapText="true" />
                    </children>
                    <padding>
                        <Insets bottom="5.0" top="5.0" />
                    </padding>
                </VBox>
                <VBox layoutX="15.0" layoutY="239.0" minHeight="40.0" prefWidth="564.0" spacing="5.0">
                    <children>
                        <Label text="MEDICATION:">
                            <styleClass>
                                <String fx:value="fw-500" />
                                <String fx:value="color-dark-2" />
                            </styleClass>
                        </Label>
                        <Label fx:id="medication" text="medication" wrapText="true" />
                    </children>
                    <padding>
                        <Insets bottom="5.0" top="5.0" />
                    </padding>
                </VBox>
                <Separator prefWidth="200.0" />
                <HBox alignment="BOTTOM_LEFT" prefHeight="40.0" prefWidth="564.0">
                    <children>
                        <Label text="Doctor's Name: " />
                        <Label prefHeight="17.0" prefWidth="163.0" text=".........................................." />
                        <Pane HBox.hgrow="ALWAYS" />
                        <Label text="Signature" />
                        <Label text="...................................." />
                    </children>
                </HBox>
            </children>
        </VBox>
    </content>
