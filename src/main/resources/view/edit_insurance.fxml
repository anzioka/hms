<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import java.lang.String?>
<VBox xmlns:fx="http://javafx.com/fxml/1" fillWidth="false" prefHeight="291.0" prefWidth="344.0" spacing="5.0"
      styleClass="bg-lighten6" stylesheets="@../css/main.css" xmlns="http://javafx.com/javafx/8"
      fx:controller="main.java.controller.EditInsuranceController">
    <children>
        <VBox styleClass="bg-lighten6" VBox.vgrow="ALWAYS">
            <children>
                <GridPane prefHeight="99.0" prefWidth="307.0" vgap="5.0">
                    <children>
                        <Label prefHeight="16.0" text="Name">
                            <styleClass>
                                <String fx:value="color-dark"/>
                                <String fx:value="fw-500"/>
                            </styleClass>
                        </Label>
                        <TextField fx:id="name" onAction="#saveDetails" prefHeight="27.0" prefWidth="266.0"
                                   GridPane.columnIndex="1"/>
                        <Label text="Group" GridPane.rowIndex="1">
                            <styleClass>
                                <String fx:value="color-dark"/>
                                <String fx:value="fw-500"/>
                            </styleClass>
                        </Label>
                        <ChoiceBox fx:id="insuranceGroupChoiceBox" prefHeight="27.0" prefWidth="298.0"
                                   GridPane.columnIndex="1" GridPane.rowIndex="1"/>
                    </children>
                    <columnConstraints>
                        <ColumnConstraints hgrow="SOMETIMES" maxWidth="163.0" minWidth="10.0" prefWidth="53.0"/>
                        <ColumnConstraints hgrow="SOMETIMES" maxWidth="255.0" minWidth="10.0" prefWidth="254.0"/>
                    </columnConstraints>
                    <rowConstraints>
                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                    </rowConstraints>
                </GridPane>
                <CheckBox fx:id="assignInsuranceCheckBox" mnemonicParsing="false" prefHeight="18.0" prefWidth="122.0"
                          text="Unassign group"/>
                <Pane prefHeight="109.0" prefWidth="200.0" VBox.vgrow="ALWAYS"/>
                <HBox alignment="CENTER_RIGHT" prefHeight="27.0" prefWidth="404.0">
                    <children>
                        <Button fx:id="deleteButton" mnemonicParsing="false" onAction="#onDelete" prefWidth="107.0"
                                styleClass="btn-danger" text="Delete" visible="false"/>
                        <Pane prefHeight="27.0" prefWidth="73.0" HBox.hgrow="ALWAYS"/>
                        <Button mnemonicParsing="false" onAction="#saveDetails" styleClass="btn-success"
                                text="Save Changes"/>
                    </children>
                </HBox>
            </children>
        </VBox>
    </children>
</VBox>
