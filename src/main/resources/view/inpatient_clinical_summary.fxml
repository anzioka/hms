<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Font?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" prefHeight="430.0" prefWidth="696.0" styleClass="scene"
            stylesheets="@../css/main.css" xmlns="http://javafx.com/javafx/8"
            fx:controller="main.java.controller.InpatientClinicalSummaryController">
    <children>
        <VBox alignment="TOP_CENTER" layoutX="5.0" layoutY="5.0" maxHeight="-Infinity" maxWidth="-Infinity"
              minHeight="-Infinity" minWidth="-Infinity" prefHeight="420.0" prefWidth="620.0" spacing="5.0"
              AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0"
              AnchorPane.topAnchor="0.0">
            <children>
                <Label styleClass="modal-title" text="Clinical Summary" underline="true"/>
                <HBox fx:id="lastModified" alignment="CENTER" prefHeight="20.0" prefWidth="686.0">
                    <children>
                        <Label styleClass="color-black" text="Last modified on ">
                            <font>
                                <Font size="12.0"/>
                            </font>
                        </Label>
                        <Label fx:id="date" styleClass="fw-500" text="date">
                            <font>
                                <Font size="12.0"/>
                            </font>
                        </Label>
                    </children>
                </HBox>
                <TextArea fx:id="editText" prefHeight="200.0" prefWidth="200.0" VBox.vgrow="ALWAYS"/>
                <HBox alignment="CENTER_RIGHT">
                    <children>
                        <Button mnemonicParsing="false" onAction="#onSave" styleClass="btn-success"
                                text="Save Changes"/>
                    </children>
                </HBox>
            </children>
        </VBox>
    </children>
</AnchorPane>
