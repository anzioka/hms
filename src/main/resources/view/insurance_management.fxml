<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import java.lang.String?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" prefHeight="545.0" styleClass="bg-lighten5"
            stylesheets="@../css/main.css" xmlns="http://javafx.com/javafx/8"
            fx:controller="main.java.controller.InsuranceManagementController">
    <children>
        <VBox alignment="CENTER" fillWidth="false" layoutX="12.0" layoutY="11.0" AnchorPane.bottomAnchor="0.0"
              AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
            <children>
                <VBox alignment="CENTER" layoutX="12.0" layoutY="12.0" maxWidth="-Infinity" prefHeight="533.0"
                      prefWidth="800.0" spacing="5.0" styleClass="bg-lighten6" AnchorPane.bottomAnchor="5.0"
                      AnchorPane.topAnchor="5.0" VBox.vgrow="ALWAYS">
                    <children>
                        <HBox alignment="CENTER" prefWidth="600.0" spacing="20.0" styleClass="bg-white">
                            <children>
                                <Button mnemonicParsing="false" onAction="#onClickInsuranceGroups" styleClass="btn-info"
                                        text="Insurance Groups"/>
                                <Button mnemonicParsing="false" onAction="#onAddNewCompany" styleClass="btn-info"
                                        text="New Company"/>
                                <Button mnemonicParsing="false" onAction="#uploadList" styleClass="btn-info"
                                        text="Upload List"/>
                                <Pane prefHeight="27.0" prefWidth="81.0" HBox.hgrow="ALWAYS"/>
                                <Label text="Search">
                                    <styleClass>
                                        <String fx:value="fw-500"/>
                                        <String fx:value="color-dark"/>
                                    </styleClass>
                                </Label>
                                <TextField fx:id="searchField"/>
                            </children>
                        </HBox>
                        <TableView fx:id="insuranceTableView" VBox.vgrow="ALWAYS">
                            <columns>
                                <TableColumn fx:id="name" prefWidth="300.0" text="Name"/>
                                <TableColumn fx:id="group" prefWidth="157.0" text="Group"/>
                                <TableColumn fx:id="options" prefWidth="142.0" text="Options"/>
                            </columns>
                        </TableView>
                    </children>
                </VBox>
            </children>
        </VBox>
    </children>
</AnchorPane>
