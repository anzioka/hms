<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.*?>
<?import java.lang.String?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" fx:id="container" prefHeight="700.0" prefWidth="882.0" styleClass="scene"
            stylesheets="@../css/main.css" xmlns="http://javafx.com/javafx/8"
            fx:controller="main.java.controller.TriageController">
    <children>
        <VBox alignment="CENTER" layoutY="54.0" prefHeight="667.0" prefWidth="700.0" spacing="7.0"
              AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0"
              AnchorPane.topAnchor="0.0">
            <children>
                <HBox alignment="CENTER" maxHeight="-Infinity" minHeight="-Infinity" prefHeight="60.0" prefWidth="200.0"
                      spacing="10.0" styleClass="bg-lighten3" VBox.vgrow="NEVER">
                    <children>
                        <ImageView fitHeight="40.0" fitWidth="40.0" pickOnBounds="true" preserveRatio="true">
                            <image>
                                <Image url="@../images/nurse.png"/>
                            </image>
                        </ImageView>
                        <Label text="Triage Station">
                            <styleClass>
                                <String fx:value="color-accent"/>
                                <String fx:value="h4"/>
                            </styleClass>
                        </Label>
                    </children>
                </HBox>
                <VBox alignment="CENTER" fillWidth="false" VBox.vgrow="ALWAYS">
                    <children>
                        <TableView fx:id="queueTableView" layoutX="23.0" layoutY="93.0" prefHeight="625.0"
                                   prefWidth="766.0" AnchorPane.leftAnchor="5.0" AnchorPane.rightAnchor="5.0"
                                   AnchorPane.topAnchor="65.0" VBox.vgrow="ALWAYS">
                            <columns>
                                <TableColumn fx:id="patientIdCol" prefWidth="75.0" text="Patient No."/>
                                <TableColumn fx:id="firstNameCol" prefWidth="75.0" text="First Name"/>
                                <TableColumn fx:id="lastNameCol" prefWidth="75.0" text="Last Name"/>
                                <TableColumn fx:id="timeCreated" prefWidth="75.0" text="Time Registered"/>
                                <TableColumn fx:id="options" prefWidth="75.0" text="Options"/>
                            </columns>
                            <columnResizePolicy>
                                <TableView fx:constant="CONSTRAINED_RESIZE_POLICY"/>
                            </columnResizePolicy>
                        </TableView>
                    </children>
                </VBox>
            </children>
        </VBox>
    </children>
</AnchorPane>
