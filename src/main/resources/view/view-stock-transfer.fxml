<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import java.lang.*?>
<VBox xmlns:fx="http://javafx.com/fxml/1" alignment="TOP_CENTER" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity"
      minWidth="-Infinity" prefHeight="465.0" prefWidth="630.0" spacing="5.0"
      stylesheets="@../css/main.css" xmlns="http://javafx.com/javafx/8"
      fx:controller="main.java.controller.ViewStockTransferController">
    <children>
        <Label fx:id="title" styleClass="modal-title" text="Stock Transfer"/>
        <VBox prefHeight="200.0" prefWidth="100.0" styleClass="bg-white" VBox.vgrow="ALWAYS">
            <children>
                <HBox alignment="CENTER_LEFT" prefHeight="41.0" prefWidth="608.0" spacing="5.0">
                    <children>
                        <Label styleClass="h7" text="Transfer No."/>
                        <Label fx:id="transferIdLabel" layoutX="10.0" layoutY="22.0" text="Label">
                            <styleClass>
                                <String fx:value="color-info-dark"/>
                                <String fx:value="fw-500"/>
                                <String fx:value="h7"/>
                            </styleClass>
                        </Label>
                        <Pane HBox.hgrow="ALWAYS"/>
                        <Label layoutX="44.0" layoutY="22.0" styleClass="h7" text="Transferred By:"/>
                        <Label fx:id="userLabel" layoutX="78.0" layoutY="22.0" text="Label">
                            <styleClass>
                                <String fx:value="color-info-dark"/>
                                <String fx:value="fw-500"/>
                                <String fx:value="h7"/>
                            </styleClass>
                        </Label>
                        <Pane HBox.hgrow="ALWAYS"/>
                        <Label layoutX="112.0" layoutY="22.0" styleClass="h7" text="Date:"/>
                        <Label fx:id="dateCreatedLabel" layoutX="146.0" layoutY="22.0" text="Label">
                            <styleClass>
                                <String fx:value="color-info-dark"/>
                                <String fx:value="fw-500"/>
                                <String fx:value="h7"/>
                            </styleClass>
                        </Label>
                    </children>
                </HBox>
                <TableView fx:id="stockTransferTableView" prefHeight="346.0" prefWidth="608.0" VBox.vgrow="ALWAYS">
                    <columns>
                        <TableColumn fx:id="drugId" prefWidth="75.0" text="Code"/>
                        <TableColumn fx:id="drugName" prefWidth="75.0" text="Name"/>
                        <TableColumn fx:id="quantity" prefWidth="75.0" text="Quantity"/>
                    </columns>
                </TableView>
                <HBox alignment="CENTER_LEFT" prefHeight="39.0" prefWidth="608.0">
                    <children>
                        <Button mnemonicParsing="false" onAction="#onClose" styleClass="btn-warning" text="Close"/>
                    </children>
                </HBox>
            </children>
        </VBox>
    </children>
    <styleClass>
        <String fx:value="scene"/>
        <String fx:value="padding-5"/>
    </styleClass>
</VBox>
