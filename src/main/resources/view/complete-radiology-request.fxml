<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Text?>
<?import java.lang.*?>

<VBox maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="500.0"
      prefWidth="669.0" spacing="5.0" stylesheets="@../css/main.css" xmlns="http://javafx.com/javafx/8"
      xmlns:fx="http://javafx.com/fxml/1" fx:controller="main.java.controller.CompleteRadiologyRequestController">
    <children>
        <HBox maxHeight="-Infinity" minHeight="-Infinity" prefHeight="60.0" prefWidth="588.0" spacing="10.0" styleClass="bg-normal">
            <children>
                <GridPane HBox.hgrow="ALWAYS">
                    <columnConstraints>
                        <ColumnConstraints hgrow="SOMETIMES" maxWidth="96.0" minWidth="10.0" prefWidth="80.0" />
                        <ColumnConstraints hgrow="SOMETIMES" maxWidth="120.0" minWidth="10.0" prefWidth="120.0" />
                    </columnConstraints>
                    <rowConstraints>
                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                    </rowConstraints>
                    <children>
                        <Label styleClass="header" text="Patient No." />
                        <Label styleClass="header" text="Sex" GridPane.rowIndex="1" />
                        <Label fx:id="patientNumber" text="Label" GridPane.columnIndex="1" />
                        <Label fx:id="sex" text="Label" GridPane.columnIndex="1" GridPane.rowIndex="1" />
                    </children>
                </GridPane>
                <GridPane HBox.hgrow="ALWAYS">
                    <columnConstraints>
                        <ColumnConstraints hgrow="SOMETIMES" maxWidth="96.0" minWidth="10.0" prefWidth="76.0" />
                        <ColumnConstraints hgrow="SOMETIMES" maxWidth="131.0" minWidth="10.0" prefWidth="131.0" />
                    </columnConstraints>
                    <rowConstraints>
                        <RowConstraints maxHeight="33.0" minHeight="10.0" prefHeight="33.0" vgrow="SOMETIMES" />
                        <RowConstraints maxHeight="36.0" minHeight="10.0" prefHeight="28.0" vgrow="SOMETIMES" />
                    </rowConstraints>
                    <children>
                        <Label styleClass="header" text="First Name" />
                        <Label styleClass="header" text="Last Name" GridPane.rowIndex="1" />
                        <Label fx:id="firstName" prefHeight="17.0" text="Label" GridPane.columnIndex="1" />
                        <Label fx:id="lastName" text="Label" GridPane.columnIndex="1" GridPane.rowIndex="1" />
                    </children>
                </GridPane>
                <GridPane HBox.hgrow="ALWAYS">
                    <columnConstraints>
                        <ColumnConstraints hgrow="SOMETIMES" maxWidth="95.0" minWidth="10.0" prefWidth="44.0" />
                        <ColumnConstraints hgrow="SOMETIMES" maxWidth="171.0" minWidth="10.0" prefWidth="165.0" />
                    </columnConstraints>
                    <rowConstraints>
                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                    </rowConstraints>
                    <children>
                        <Label styleClass="header" text="DOB" />
                        <Label styleClass="header" text="Age" GridPane.rowIndex="1" />
                        <Label fx:id="dateOfBirth" text="Label" GridPane.columnIndex="1" />
                        <Label fx:id="age" text="Label" GridPane.columnIndex="1" GridPane.rowIndex="1" />
                    </children>
                </GridPane>
            </children>
        </HBox>
        <HBox alignment="CENTER_LEFT" maxHeight="-Infinity" minHeight="-Infinity" prefHeight="30.0" prefWidth="659.0" spacing="5.0">
            <children>
                <Label fx:id="testName" text="X-Ray(Arm)">
                    <styleClass>
                        <String fx:value="fw-500" />
                        <String fx:value="color-teal" />
                        <String fx:value="h6_5" />
                    </styleClass>
                </Label>
                <Pane HBox.hgrow="ALWAYS" />
            <Label text="Date" />
                <Label fx:id="date" styleClass="fw-500" text="Label" />
            <Pane layoutX="93.0" layoutY="10.0" HBox.hgrow="ALWAYS" />
                <Label text="Doctor" />
                <Label fx:id="requester" styleClass="fw-500" text="Label" />
            </children>
        </HBox>
        <Separator prefWidth="200.0" />
        <VBox prefHeight="90.0" prefWidth="659.0" spacing="5.0" styleClass="bg-white" VBox.vgrow="ALWAYS">
            <children>
                <Label styleClass="color-dark-4" text="Results" />
                <TextArea fx:id="notes" prefHeight="65.0" prefWidth="647.0" promptText="Enter result"
                          style="-fx-font-family: &quot;Lucida Sans Typewriter&quot;;" VBox.vgrow="ALWAYS" />
            </children>
        </VBox>
        <HBox alignment="CENTER_RIGHT" maxHeight="-Infinity" minHeight="-Infinity" prefHeight="40.0" prefWidth="657.0" spacing="10.0" styleClass="bg-lighten5">
            <children>
                <!--<Button mnemonicParsing="false" onAction="#onPrintLabResults" styleClass="btn-info-outline" text="Print Results" visible="false" />-->
                <Pane prefHeight="40.0" HBox.hgrow="ALWAYS" />
                <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Results Submitted by:" />
                <Label fx:id="currentUser" styleClass="header-light-bg" text="Label" />
                <Button mnemonicParsing="false" onAction="#onSubmitResults" styleClass="btn-info" text="Submit Results" />
            </children>
        </HBox>
    </children>
    <styleClass>
        <String fx:value="container-padded" />
        <String fx:value="scene" />
    </styleClass>
</VBox>
