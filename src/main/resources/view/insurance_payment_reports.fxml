<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.VBox?>
<VBox xmlns:fx="http://javafx.com/fxml/1" alignment="TOP_CENTER" spacing="5.0" styleClass="scene"
      stylesheets="@../css/main.css" xmlns="http://javafx.com/javafx/8">
    <children>
        <VBox prefHeight="84.0" prefWidth="764.0" styleClass="border-light-3"/>
        <TableView fx:id="invoiceTableView" prefHeight="400.0" prefWidth="764.0" VBox.vgrow="ALWAYS">
            <columnResizePolicy>
                <TableView fx:constant="CONSTRAINED_RESIZE_POLICY"/>
            </columnResizePolicy>
            <columns>
                <TableColumn fx:id="invoiceNumber" prefWidth="118.0" text="Invoice No."/>
                <TableColumn fx:id="patientNo" prefWidth="119.0" text="Patient No."/>
                <TableColumn fx:id="patientName" prefWidth="239.0" style="-fx-alignment: center-left;"
                             text="Patient Name"/>
                <TableColumn fx:id="insuranceCo" prefWidth="75.0" style="-fx-alignment: center-left;"
                             text="Insurance Co."/>
                <TableColumn fx:id="invoiceAmount" prefWidth="163.0" text="Amount  (Ksh)"/>
                <TableColumn fx:id="invoiceDate" prefWidth="154.0" text="Date"/>
                <TableColumn fx:id="options" prefWidth="166.0" text="Options"/>
            </columns>
        </TableView>
    </children>
    <padding>
        <Insets top="10.0"/>
    </padding>
</VBox>
