<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Tab?>
<?import javafx.scene.control.TabPane?>
<?import javafx.scene.layout.*?>
<?import java.lang.String?>
<AnchorPane styleClass="scene" stylesheets="@../css/main.css" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="main.java.controller.WardManagementController">
    <children>
        <TabPane fx:id="tabPane" layoutX="5.0" layoutY="5.0" prefHeight="468.0" prefWidth="635.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
            <tabs>
                <Tab closable="false" text="Wards">
                    <content>
                        <VBox layoutX="5.0" layoutY="5.0" spacing="5.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                            <children>
                                <Button fx:id="createWardBtn" mnemonicParsing="false" onAction="#onCreateNewWard" styleClass="btn-info-outline" text="Create New Ward" />
                                <HBox alignment="CENTER" />
                                <FlowPane fx:id="flowPane" alignment="CENTER" hgap="20.0" prefHeight="200.0" prefWidth="200.0" vgap="20.0" VBox.vgrow="ALWAYS">
                                    <styleClass>
                                        <String fx:value="border-light-3" />
                                        <String fx:value="container-padded" />
                                    </styleClass>
                                </FlowPane>
                            </children>
                            <padding>
                                <Insets top="5.0" />
                            </padding>
                        </VBox>
                    </content>
                </Tab>
            </tabs>
        </TabPane>
    </children>
</AnchorPane>
