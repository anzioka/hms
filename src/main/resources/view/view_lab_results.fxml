<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.*?>
<VBox xmlns:fx="http://javafx.com/fxml/1" fx:id="container" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity"
      minWidth="-Infinity" prefHeight="579.0" prefWidth="674.0" spacing="10.0" styleClass="container-padded"
      stylesheets="@../css/main.css" xmlns="http://javafx.com/javafx/8"
      fx:controller="main.java.controller.ViewLabResultsController">
    <children>
        <HBox alignment="CENTER_RIGHT" prefHeight="52.0" prefWidth="704.0" spacing="5.0">
            <children>
                <ImageView fx:id="letterHead" fitHeight="75.0" fitWidth="664.0" pickOnBounds="true"/>

                <Pane fx:id="pane" prefHeight="200.0" prefWidth="200.0"/>
            </children>
        </HBox>
        <HBox prefHeight="70.0" prefWidth="780.0" spacing="20.0" styleClass="bg-lighten6">
            <children>
                <GridPane layoutX="-37.0" layoutY="28.0" prefHeight="70.0" prefWidth="187.0" HBox.hgrow="ALWAYS">
                    <children>
                        <Label styleClass="header" text="Patient ID"/>
                        <Label styleClass="header" text="Sex" GridPane.rowIndex="1"/>
                        <Label fx:id="patientId" opacity="0.9" text="Label" GridPane.columnIndex="1"/>
                        <Label fx:id="sex" opacity="0.9" text="Label" GridPane.columnIndex="1" GridPane.rowIndex="1"/>
                    </children>
                    <columnConstraints>
                        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
                        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
                    </columnConstraints>
                    <rowConstraints>
                        <RowConstraints maxHeight="37.0" minHeight="10.0" prefHeight="34.0" vgrow="SOMETIMES"/>
                        <RowConstraints maxHeight="36.0" minHeight="10.0" prefHeight="36.0" vgrow="SOMETIMES"/>
                    </rowConstraints>
                </GridPane>
                <GridPane layoutX="220.0" layoutY="28.0" HBox.hgrow="ALWAYS">
                    <children>
                        <Label styleClass="header" text="First Name"/>
                        <Label styleClass="header" text="Surname" GridPane.rowIndex="1"/>
                        <Label fx:id="firstName" opacity="0.9" text="Label" GridPane.columnIndex="1"/>
                        <Label fx:id="lastName" opacity="0.9" text="Label" GridPane.columnIndex="1"
                               GridPane.rowIndex="1"/>
                    </children>
                    <columnConstraints>
                        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
                        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
                    </columnConstraints>
                    <rowConstraints>
                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                    </rowConstraints>
                </GridPane>
                <GridPane layoutX="452.0" layoutY="35.0" HBox.hgrow="ALWAYS">
                    <children>
                        <Label styleClass="header" text="Date of Birth"/>
                        <Label styleClass="header" text="Age" GridPane.rowIndex="1"/>
                        <Label fx:id="dateOfBirth" opacity="0.9" text="Label" GridPane.columnIndex="1"/>
                        <Label fx:id="age" opacity="0.9" text="Label" GridPane.columnIndex="1" GridPane.rowIndex="1"/>
                    </children>
                    <columnConstraints>
                        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
                        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
                    </columnConstraints>
                    <rowConstraints>
                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                    </rowConstraints>
                </GridPane>
            </children>
        </HBox>
        <TableView fx:id="tableView" prefHeight="200.0" prefWidth="200.0" VBox.vgrow="ALWAYS">
            <columns>
                <TableColumn fx:id="specimen" prefWidth="75.0" text="Specimen"/>
                <TableColumn fx:id="testName" prefWidth="277.0" text="Test"/>
                <TableColumn fx:id="result" prefWidth="311.0" text="Result"/>
            </columns>
        </TableView>
        <HBox fx:id="buttonBar" alignment="CENTER_RIGHT" prefHeight="38.0" prefWidth="664.0">
            <children>
                <Button mnemonicParsing="false" onAction="#onPrintResults" styleClass="btn-info" text="Print Results"/>
            </children>
        </HBox>
    </children>
</VBox>
