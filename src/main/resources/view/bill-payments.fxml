<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import java.lang.String?>
<VBox xmlns:fx="http://javafx.com/fxml/1" fx:id="container" prefHeight="400.0" prefWidth="736.0" spacing="5.0"
      styleClass="scene" stylesheets="@../css/main.css" xmlns="http://javafx.com/javafx/8"
      fx:controller="main.java.controller.BillPaymentsController">
    <children>
        <HBox alignment="CENTER_LEFT" prefHeight="34.0" prefWidth="726.0" spacing="5.0">
            <children>
                <Label styleClass="h6_5" text="Total Payments : "/>
                <Label fx:id="totalPayments" text="Label">
                    <styleClass>
                        <String fx:value="h6_5"/>
                        <String fx:value="color-unique"/>
                        <String fx:value="fw-500"/>
                    </styleClass>
                </Label>
            </children>
        </HBox>
        <TableView fx:id="tableView" prefHeight="200.0" prefWidth="200.0" VBox.vgrow="SOMETIMES">
            <columns>
                <TableColumn fx:id="receiptNo" prefWidth="75.0" text="Receipt No."/>
                <TableColumn fx:id="date" prefWidth="75.0" text="Date"/>
                <TableColumn fx:id="time" prefWidth="75.0" text="Time"/>
                <TableColumn fx:id="amount" prefWidth="75.0" text="Amount Paid"/>
                <TableColumn fx:id="receivedBy" prefWidth="75.0" text="Received By"/>
                <TableColumn fx:id="paymentMeans" prefWidth="75.0" text="Payment Means"/>
                <TableColumn fx:id="accountAffected" prefWidth="75.0" text="Account Affected"/>
                <TableColumn fx:id="options" prefWidth="75.0" text="Options"/>
            </columns>
            <columnResizePolicy>
                <TableView fx:constant="CONSTRAINED_RESIZE_POLICY"/>
            </columnResizePolicy>
        </TableView>
    </children>
</VBox>
